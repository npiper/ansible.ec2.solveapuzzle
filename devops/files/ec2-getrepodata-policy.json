{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ec2-get-repoassets",
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": [
                "arn:aws:s3:::snapshot",
                "arn:aws:s3:::repository"
            ]
        }
    ]
}
